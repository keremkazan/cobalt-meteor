import { FlowRouter } from 'meteor/kadira:flow-router';
import {mount} from 'react-mounter';
import React from 'react';
import <%= layoutName %> from '../../imports/ui/Layouts/<%= layoutName %>.jsx';

<%_ routes.forEach(function (route) { %>import <%= route.page %> from '<%= route.pageFilepath %>';
<%_ }); %>
const <%= sectionName %> = FlowRouter.group({
  prefix: '/<%= prefix %>',
});
<% routes.forEach(function (route) { %>
<%= sectionName %>.route('<%= prefix %>', {
  name: 'widgets.<%= route.prefix %>',
  action: (params) => {
    mount(<%= layoutName %>, {
      content: (<<%= route.page %> />)
    });
  },
});
<% }); %>
